    "from bokeh.plotting import figure, show\n",
    "from bokeh.io import output_notebook, push_notebook\n",
    "from bokeh.palettes import Spectral4\n",
    "\n",
    "def bokeh_plot(y, **kwargs):\n",
    "    output_notebook()\n",
    "    p = figure()\n",
    "    p.line(range(len(y)), y, **kwargs)\n",
    "    show(p)"
